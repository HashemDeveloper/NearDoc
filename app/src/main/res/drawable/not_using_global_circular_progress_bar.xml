<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
   <item android:id="@+id/secondaryProgressId">
       <shape
           android:innerRadiusRatio="6"
           android:shape="ring"
           android:thicknessRatio="20.0"
           android:useLevel="true"
           >
           <gradient
               android:centerColor="@color/blue_gray_100"
               android:endColor="@color/blue_gray_800"
               android:startColor="@color/blue_gray_300"
               android:type="sweep"
               />
       </shape>
   </item>
    <item android:id="@+id/primaryProgressId">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270"
            >
            <shape
                android:innerRadiusRatio="6"
                android:shape="ring"
                android:thicknessRatio="20.0"
                android:useLevel="true"
                >
                <rotate
                    android:fromDegrees="270"
                    android:pivotX="50%"
                    android:pivotY="50%"
                    android:toDegrees="360"
                    />
                <gradient
                    android:centerColor="@color/blue_gray_400"
                    android:endColor="@color/blue_gray_400"
                    android:startColor="@color/blue_gray_400"
                    android:type="sweep"
                    />
            </shape>
        </rotate>
    </item>
</layer-list>